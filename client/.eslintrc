{
  "env": {
    "browser": true,
    "amd": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["import", "prettier"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error"
  }
}
